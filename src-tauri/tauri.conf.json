{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "neurois",
  "version": "0.1.0",
  "identifier": "com.neurois.app",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devUrl": "http://127.0.0.1:7999",
    "frontendDist": "http://127.0.0.1:7999"
  },
  "app": {
    "withGlobalTauri": true,
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "neurois",
        "width": 800,
        "height": 600,
        "transparent": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "resources": [
      "apps",
      "data",
      "scripts"
    ],
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
